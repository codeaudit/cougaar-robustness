log4j.rootCategory=WARN,Ax

# Sensors
log4j.category.org.cougaar.tools.robustness.sensors=INFO

# Msg*Log
log4j.category.org.cougaar.core.mts.acking=INFO
log4j.category.org.cougaar.core.mts.AdaptiveLinkSelectionPolicy=INFO
#log4j.category.org.cougaar.core.mts.AdaptiveLinkSelectionPolicy=DEBUG
#log4j.category.org.cougaar.core.mts.ManagedNameSupportAspect=DEBUG
#log4j.category.org.cougaar.core.mts.RTTAspect=DEBUG
#log4j.category.org.cougaar.core.mts.acking=DEBUG
#log4j.category.org.cougaar.core.mts.email=DEBUG
#log4j.category.org.cougaar.core.mts.socket=DEBUG
#log4j.category.org.cougaar.core.mts.udp=DEBUG

# misc
#log4j.category.org.cougaar.core.adaptivity.MetricsTestPlugin=INFO
#log4j.category.org.cougaar.core.mobility=DEBUG

# console appender
log4j.appender.Ax=org.apache.log4j.ConsoleAppender
log4j.appender.Ax.Target=System.out

# file appender
#log4j.appender.Ax=org.apache.log4j.FileAppender
#log4j.appender.Ax.File=node.log

# format
log4j.appender.Ax.layout=org.apache.log4j.PatternLayout
log4j.appender.Ax.layout.ConversionPattern=%n%d{ABSOLUTE} %-5p %c{1} %m
